# Paymob Payment Gateway Configuration
# Get these from your Paymob Dashboard: https://accept.paymob.com/portal2/en/settings

# Your API Key (from Settings > Account Info > API Key)
VITE_PAYMOB_API_KEY=your_api_key_here

# Card Payment Integration ID (from Settings > Integration ID for Card payments)
VITE_PAYMOB_INTEGRATION_ID_CARD=your_card_integration_id_here

# Wallet Payment Integration ID (from Settings > Integration ID for Mobile Wallets)
VITE_PAYMOB_INTEGRATION_ID_WALLET=your_wallet_integration_id_here

# iFrame ID (from Settings > iFrames)
VITE_PAYMOB_IFRAME_ID=your_iframe_id_here
